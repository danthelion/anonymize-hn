{
  "name": "Anonymize HN",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Anonymize Hacker News users",
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://news.ycombinator.com/*"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}